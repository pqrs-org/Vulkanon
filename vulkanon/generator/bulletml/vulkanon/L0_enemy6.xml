<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">
<bulletml>
  <action label="top">
    <repeat>
      <times>1000</times>
      <action>
        <fire>
          <direction type="aim">10</direction>
          <speed type="absolute">4</speed>
          <bullet />
        </fire>
        <fire>
          <direction type="aim">-10</direction>
          <speed type="absolute">4</speed>
          <bullet />
        </fire>
        <wait>1</wait>
      </action>
    </repeat>
  </action>
</bulletml>
