<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">
<bulletml>
  <action label="top">
    <changeDirection>
      <direction type="absolute">$rand*360</direction>
      <term>1</term>
    </changeDirection>
    <wait>2</wait>
    <changeDirection>
      <direction type="sequence">-3</direction>
      <term>9999</term>
    </changeDirection>
    <changeSpeed>
      <speed type="absolute">4</speed>
      <term>100</term>
    </changeSpeed>
    <repeat>
      <times>50</times>
      <action>
        <fire>
          <direction type="relative">90</direction>
          <speed type="absolute">2</speed>
          <bullet />
        </fire>
        <fire>
          <direction type="relative">-90</direction>
          <speed type="absolute">2</speed>
          <bullet />
        </fire>
        <wait>3</wait>
      </action>
    </repeat>
    <vanish />
  </action>
</bulletml>
