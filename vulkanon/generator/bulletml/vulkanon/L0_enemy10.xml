<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">
<bulletml>
  <action label="top">
    <changeDirection>
      <direction type="absolute">180</direction>
      <term>1</term>
    </changeDirection>
    <changeSpeed>
      <speed type="absolute">1</speed>
      <term>1</term>
    </changeSpeed>
    <wait>20</wait>
    <repeat>
      <times>10</times>
      <action>
        <actionRef label="fireLine">
          <param>130</param>
        </actionRef>
        <actionRef label="fireLine">
          <param>150</param>
        </actionRef>
        <actionRef label="fireLine">
          <param>170</param>
        </actionRef>
        <actionRef label="fireLine">
          <param>190</param>
        </actionRef>
        <actionRef label="fireLine">
          <param>210</param>
        </actionRef>
        <actionRef label="fireLine">
          <param>230</param>
        </actionRef>
        <wait>30</wait>
      </action>
    </repeat>
  </action>
  <action label="fireLine">
    <fire>
      <direction type="absolute">$1</direction>
      <speed type="absolute">2</speed>
      <bullet />
    </fire>
    <actionRef label="fireLineHoming">
      <param>$1</param>
    </actionRef>
    <actionRef label="fireLineHoming">
      <param>$1</param>
    </actionRef>
    <actionRef label="fireLineHoming">
      <param>$1</param>
    </actionRef>
    <actionRef label="fireLineHoming">
      <param>$1</param>
    </actionRef>
    <actionRef label="fireLineHoming">
      <param>$1</param>
    </actionRef>
  </action>
  <action label="fireLineHoming">
    <fire>
      <direction type="absolute">$1</direction>
      <speed type="sequence">0.25</speed>
      <bullet />
    </fire>
  </action>
</bulletml>
